CREATE TABLE session(
    id int NOT NULL PRIMARY KEY AUTO_INCREMENT,
    session varchar(255)
);

CREATE TABLE section(
    id int NOT NULL PRIMARY KEY AUTO_INCREMENT,
    section varchar(255)
);

CREATE TABLE subject(
    id int NOT NULL PRIMARY KEY AUTO_INCREMENT,
    sub_name varchar(255),
    sub_code varchar(255)
);

CREATE TABLE student(
    id int NOT NULL PRIMARY KEY AUTO_INCREMENT,
        name varchar(255),
        email varchar(255),
        password varchar(255),
        role varchar(255)
);

